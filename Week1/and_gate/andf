#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5578d06850d0 .scope module, "and_test" "and_test" 2 1;
 .timescale 0 0;
v0x5578d06a97c0_0 .var "a", 0 0;
v0x5578d06a9890_0 .var "b", 0 0;
v0x5578d06a9960_0 .net "y", 0 0, L_0x5578d06a9a60;  1 drivers
S_0x5578d0696be0 .scope module, "and_test" "andgate" 2 4, 3 1 0, S_0x5578d06850d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5578d06a9a60 .functor AND 1, v0x5578d06a97c0_0, v0x5578d06a9890_0, C4<1>, C4<1>;
v0x5578d065fc00_0 .net "a", 0 0, v0x5578d06a97c0_0;  1 drivers
v0x5578d06a95e0_0 .net "b", 0 0, v0x5578d06a9890_0;  1 drivers
v0x5578d06a96a0_0 .net "y", 0 0, L_0x5578d06a9a60;  alias, 1 drivers
    .scope S_0x5578d06850d0;
T_0 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5578d06a97c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5578d06a9890_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5578d06a97c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5578d06a9890_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5578d06a97c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5578d06a9890_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5578d06a97c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5578d06a9890_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5578d06850d0;
T_1 ;
    %vpi_call 2 16 "$monitor", $time, "a = %b, y = %b, y = %b", v0x5578d06a97c0_0, v0x5578d06a9890_0, v0x5578d06a9960_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5578d06850d0;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "and_gate.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5578d0696be0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "andtb.v";
    "and.v";
